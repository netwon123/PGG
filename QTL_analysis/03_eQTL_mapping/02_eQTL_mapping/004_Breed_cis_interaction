for K in `seq 2 3`
do
admixture --cv SNP-INDEL_rmMiss.prune.bed -j36  $K |tee log${K}.out
done


seq 1 18 | xargs -I {} -P 8 bash -c '
    OmiGA --mode cis interactlon \
        --genotype ../02_genotype/joint \
        --dcovar-name sex \
        --chrom {} \
		--no-add-interaction-to-covar \
		--interaction-name LW \
        --phenotype ../01_phenotype/omgia_duodenum.expression.bed.gz \
        --prefix joint \
        --covariates ../03_covariant/Cov_duodenum.txt \
        --output-dir duodenum_lw_cis_interaction \
        --threads 7 \
		--interaction breed_cov.txt \
		--maf-threshold-interaction 0.01 \
        --calcu-variant-threshold
'

OmiGA --mode cis_mt \
 --threads 36 \
 --cis-file liver_cis_eqtl_joint/joint.cis_qtl.txt.gz \
 --multiple-testing acat \
 --prefix joint \
 --output-dir liver_cis_eqtl_joint_mt
